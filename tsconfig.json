{
  "compileOnSave": false,
  "compilerOptions": {
    "downlevelIteration": true,
    "importHelpers": true,
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
		"lib": [
			"dom",
			"es2020"
		],
    "noUnusedLocals": true,
    "noUnusedParameters": false,
    "baseUrl": "./",
    "module": "ES2020"
  },
	"angularCompilerOptions": {
		"preserveWhitespaces": true
	}
}
